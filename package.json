{
  "name": "newApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --coverage",
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "prebuild": "node node_modules/.bin/rimraf build",
    "build": "npm run clean && npm run tsc --",
    "lint": "tslint src/**/*.ts",
    "watch": "npm run build -- -w",
    "start:ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
    "start:android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'"
  },
  "dependencies": {
    "react": "16.0.0-alpha.12",
    "react-native": "0.46.4",
    "react-native-code-push": "^4.0.0-beta"
  },
  "devDependencies": {
    "@types/jest": "^20.0.4",
    "@types/react": "^15.0.38",
    "@types/react-native": "^0.46.3",
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.1.0",
    "concurrently": "^3.5.0",
    "jest": "20.0.4",
    "react-test-renderer": "16.0.0-alpha.12",
    "rimraf": "^2.6.1",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
